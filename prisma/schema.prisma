// prisma/schema.prisma
datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id           String  @id @default(cuid())
  first_name   String
  last_name    String
  email        String  @unique
  password     String
  country_code String
  phone_no     String
  address_1    String
  address_2    String
  postcode     String
  state        String
  city         String?
}
